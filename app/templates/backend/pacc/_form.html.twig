{{ form_start(form) }}
<div class="row">
    <div class="col-xl-6 mb-3">
        {{ form_label(form.dateDebut) }}
        {{ form_widget(form.dateDebut) }}
    </div>
    <div class="col-xl-6 mb-3">
        {{ form_label(form.dateFin) }}
        {{ form_widget(form.dateFin) }}
    </div>
    <div class="col-xl-6 mb-3">
        {{ form_label(form.cheminFichier) }}
        {{ form_widget(form.cheminFichier) }}
    </div>
    <div class="col-xl-6 mb-3">
        {{ form_label(form.nomFichier) }}
        {{ form_widget(form.nomFichier) }}
    </div>
    <div class="col-xl-6 mb-3">
        {{ form_label(form.mandatCoges) }}
        {{ form_widget(form.mandatCoges) }}
    </div>
    {{ form_row(form._token) }}
    <hr>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary btn-sm me-1"><i id="btn-spin" class="fa fa-spinner fa-spin fa-1x"></i> Enregister</button>
        <span class="btn btn-info btn-sm light ms-1" id="btnCancel"  data-bs-dismiss="offcanvas">Annuler</span>
    </div>
</div>
{{ form_end(form, {'render_rest': false}) }}
